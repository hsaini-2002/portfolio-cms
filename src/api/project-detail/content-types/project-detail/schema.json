    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid"
    },
    "name": {
      "type": "string"
    },
    "industry": {
      "type": "string"
    },
    "about": {
      "type": "blocks"
    },
    "services": {
