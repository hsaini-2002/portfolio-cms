      "enum": [
        "Branding",
        "Packaging",
        "UI/UX"
      ]
    },
    "hero": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "challenge": {
      "type": "blocks"
    },
    "solution": {
      "type": "blocks"
    },
    "outcome": {
      "type": "blocks"
    }
  }
}
